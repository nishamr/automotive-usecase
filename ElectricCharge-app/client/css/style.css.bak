.green {color:#57a975;}
.orange{color:orange;}
.dashboard_drk_right label {
	display: block;
	font-weight:normal;
	font-size: 18px;
	color:#888888;
}
.dashboard_drk_right input[type="text"] {
	display: block;
	margin-bottom: 20px;
	border-radius: 5px;
	border: none;
	padding: 10px 15px;
	font-size: 18px;
	width: 100%;
	background: #252938;
	color: #9a9a9a;
}
.dashboard_drk_right input[type="date"] {
	display: block;
	margin-bottom: 20px;
	border-radius: 5px;
	border: none;
	padding: 10px 15px;
	font-size: 18px;
	width: 80%;
	background: #252938;
	color: #9a9a9a;
}
.dashboard_drk_right input[type="submit"] {
	background: #f26522;
	border: 1px solid #f26522;
	color: #FFF;
	border-radius: 50px;
	padding: 8px 20px;
	width: 150px;
	font-size: 16px;
	margin: 15px;
}
.dashboard_drk_right input[type="submit"]:hover {
	background: #FFF;
	border: 1px solid #f26522;
	color: #f26522;
}
.dashboard_drk_right .wht input[type="text"] {
	display: block;
	margin-bottom: 20px;
	border-radius: 5px;
	border: none;
	padding: 10px 15px;
	font-size: 18px;
	width: 80%;
	background: #FFFFFF;
}
.dashboard_drk_right .wht input[type="date"] {
	display: block;
	margin-bottom: 20px;
	border-radius: 5px;
	border: none;
	padding: 10px 15px;
	font-size: 18px;
	width: 80%;
	background: #FFFFFF;
}
.dashboard h1 {
	padding: 20px 30px;
	color: #0072bc;
	text-transform: uppercase;
	font-size: 24px;
	font-weight: bold;
}
.dashboard_drk h1 {
	padding: 20px 30px;
	color: #f26522;
	text-transform: uppercase;
	font-size: 24px;
	font-weight: bold;
}
.accordion, .accordion_1 {
	background: #e6eef1;
	text-transform: uppercase;
	cursor: pointer;
	padding: 10px 0px;
	width: 100%;
	border: none;
	text-align: left;
	outline: none;
	font-size: 14px;
	transition: 0.4s;
}
.active, .accordion:hover, .accordion_1:hover {
	color: #39b54a;
}
.accordion:after {
	content: '\02C3';
	font-weight: bold;
	float: right;
	margin-left: 5px;
}
.active:after {
	content: "\02C5";
}
.dashboard .left_menu {
	margin: 10px 30px;
}
.dashboard_drk .left_menu {
	margin: 30px 30px 20px 30px;
}
.dashboard_drk .left_menu li:nth-child(1)  {
	width:47%; margin: 0px; background: #0c88ea; margin-right: 6%; 
}
.dashboard_drk .left_menu li {
	width:47%; margin: 0px; background: #f06eaa; float: left; height:135px ;text-align: center;
	text-transform: uppercase; padding-top: 0px; 
}
.dashboard_drk .left_menu li a {
	color:#FFFFFF;
}
.dashboard_drk .left_menu li a:hover {
	color:#FFFFFF; text-decoration: none;
}
.dashboard_drk .left_submenu {
	background: #2f3343;
	text-transform: uppercase;
	cursor: pointer;
	width: 100%;
	border: none;
	text-align: left;
	outline: none;
	font-size: 14px;
	padding: 20px 30px  0px 30px;
}
.dashboard .left_submenu {
	text-transform: uppercase;
	cursor: pointer;
	width: 100%;
	border: none;
	text-align: left;
	outline: none;
	font-size: 14px;
	padding: 20px 30px 0px 30px;
}
.dashboard_drk .left_submenu  input[type="text"], .dashboard .left_submenu  input[type="text"] {
	float:left; padding:5px 10px; border:0px; color:#AAAAAA;
}

.dashboard_drk .left_submenu  input[type="submit"], .dashboard .left_submenu  input[type="submit"] {
	float:right; padding:5px 10px; border:0px; background: url(../images/icon_11.gif) no-repeat center center;
}

.dashboard .left_submenu li, .dashboard_drk .left_submenu li {
	padding: 5px 5px;
}
.dashboard .left_submenu li:nth-child(1) {
	border-bottom: 0px solid #7daca9;
}
.dashboard_drk .left_submenu li:nth-child(1) {
	border-bottom: 0px solid #252938;
}
.search_bar {background: #FFFFFF; border-radius: 5px;-moz-box-shadow:    inset 0 0 5px #aaaaaa;
   -webkit-box-shadow: inset 0 0 5px #aaaaaa;
   box-shadow:         inset 0 0 5px #aaaaaa; border:1px solid #FFFFFF;}

.create_light {border-radius:5px; background: #0072bc; text-align: center; margin-bottom: 15px; color:#FFFFFF; }
.create_light:hover {background: #2691d6;}
.create_dark {border-radius:5px; background: #f26522; text-align: center; margin-bottom: 15px; color:#FFFFFF; }
.create_dark:hover {border-radius:5px; background: #eb7e4b; text-align: center; margin-bottom: 15px; color:#FFFFFF; }

.panel {
	padding: 0px 0px;
	background-color: #e6e6e6;
	margin: 0px;
	border: 0px;
	max-height: 0;
	overflow: hidden;
	transition: max-height 0.2s ease-out;
	border-radius: 0px;
}
.panel ul {
	border-top: 2px solid #39b54a;
	padding: 10px 20px;
}
.panel ul li {
	padding: 5px 0px;
}
.panel ul li a {
	color: #333333;
	text-decoration: none;
}
.panel ul li a:hover {
	color: #39b54a;
	text-decoration: none;
}

.dashboard_right ul { margin: 10px 15px; }
.dashboard_right ul li { background: #d5d6d8; padding: 20px 20px; border-bottom: 1px solid #FAFAFA; margin-bottom: 10px; font-size: 16px;}
.dashboard_right ul li a { color:#0c88ea; text-decoration: underline; font-weight: bold;}
.dashboard_right ul li .status { float: right;}
.dashboard_right ul li img { margin-left: 20px;}
.dashboard_right h3 {
	margin: 15px 15px 20px 15px;
}
.dashboard_right label {
	display: block;
	font-size: 18px;
}
.dashboard_right ul li span { display:inline; font-size: 16px;}
.dashboard_right table td, .dashboard_right table th {padding: 10px 10px; border-bottom: 1px solid #b4d3e0;}
.dashboard_drk_right table td,.dashboard_drk_right table th {padding: 10px 10px; border-bottom: 1px solid #252938;}
.dashboard_right table th {background:#b4d3e0;}
.dashboard_drk_right table th {background:#252938;}
.dashboard_drk_right label.labelvalue { font-size: 18px; color:#FFFFFF; font-weight: bold; } 
.dashboard_right label.labelvalue { font-size: 18px; color:#000000; font-weight: bold; }

.message-box { width:80%; border:1px solid #519cfb; border-radius:5px; padding:10px 20px; margin-bottom:20px; background:#cee5fd; color:#000000; }
.message-box h3 { margin-left:0px; margin-right:0px; color:#519cfb; }
.message-box p, .alert-box p { margin-bottom:20px; }
.message-box  input[type="button"] { margin-bottom:20px; background: #519cfb; border: 0px solid #0072bc; color: #FFF; border-radius: 5px; padding: 5px 15px; width: 100px; font-size: 16px; margin-right:5px; }


.alert-box { width:80%; border:1px solid #d96473; border-radius:5px; padding:10px 20px; margin-bottom:20px; background:#f4d6d9; color:#000000; }
.alert-box h3 { margin-left:0px; margin-right:0px; color:#d96473; }
.alert-box  input[type="button"]  { margin-bottom:20px; background: #d96473; border: 0px solid #0072bc; color: #FFF; border-radius: 5px; padding: 5px 15px; width: 100px; font-size: 16px; margin-right:5px; }
.alert {
	padding: 20px;
	background-color: #d2f8d2;
	color: #093b09;
  }
  .alertWarning {
	padding: 20px;
	background-color: #FFCC11;
	color: #B13E0F;
  }
  .closebtn {
	margin-left: 15px;
	color: #093b09;
	font-weight: bold;
	float: right;
	font-size: 22px;
	line-height: 20px;
	cursor: pointer;
	transition: 0.3s;
  }

  .detailsarrow {
	margin-left: 5px;
	color: #093b09;
	font-weight: bold;
	font-size: 22px;
	line-height: 20px;
	cursor: pointer;
	transition: 0.3s;
  }
  
  .detailsarrow:hover {
	margin-left: 10px;
  }
  .closebtn:hover {
	color: black;
  }

  .advertisement{
	margin-left: 5px;
	color:white;
	font-weight: bold;
	font-size: 22px;
	line-height: 20px;
	cursor: pointer;
	transition: 0.3s;
  }

  .banned{
	  cursor: not-allowed;
  }

  .error{
	color:red;
	font-size: 8px;
  }

  .clearfix:before,
  .clearfix:after{
	  content:" ";
	  display: table;
  }

  .clearfix:after{
	  clear:both;
  }
  .loader {
	border: 16px solid #f3f3f3;
	border-radius: 50%;
	border-top: 16px solid #3498db;
	width: 120px;
	height: 120px;
	-webkit-animation: spin 2s linear infinite; /* Safari */
	animation: spin 2s linear infinite;
  }

  .numberRight{
    text-align: right;
}

.dollar{
	display: inline-block;
}
  
  /* Safari */
  @-webkit-keyframes spin {
	0% { -webkit-transform: rotate(0deg); }
	100% { -webkit-transform: rotate(360deg); }
  }
  
  @keyframes spin {
	0% { transform: rotate(0deg); }
	100% { transform: rotate(360deg); }
  }