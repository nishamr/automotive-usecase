.home {
	padding: 200px 0 0 0;
	border-radius: 0px;
	width: 800px;
	margin: auto;
	background: url(../images/bg_home.png) no-repeat center center;
	min-height: 200px;
}
.supplier_btn {
	background: #f26522;
	border: 1px solid #f26522;
}
.supplier_btn:hover {
	background: #FFF;
	border: 1px solid #f26522;
	color: #f26522;
}
.schneider_btn {
	background: #0072bc;
	border: 1px solid #0072bc;
}
.schneider_btn:hover {
	background: #FFF;
	border: 1px solid #0072bc;
	color: #0072bc;
}
.breadcrumb_main {
	background: #40474f;
	padding: 15px 20px;
}
.breadcrumb_main a {
	color: #FFFFFF;
	text-decoration: none;
	font-size: 18px;
}
.breadcrumb_main .left {
	width: 20%;
	float: left;
	text-align: left;
}
.breadcrumb_main .center {
	width: 60%;
	float: left;
	text-align: center;
	font-size: 25px;
	font-weight: bold;
	color:#ffffff;
}
.breadcrumb_main .right {
	width: 20%;
	float: right;
