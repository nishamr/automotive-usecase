	width: 100%;
	position: relative;
	color: #FFFFFF;
}
.dashboard_drk .user {
	background: url(../images/icon_user.png) no-repeat 30px 10px #252938;
	height: 92px;
	padding: 20px 0px 15px 110px;
	font-size: 12px;
	color:#9a9a9a;
}
.dashboard_drk_left span {
	font-size: 20px;
	font-weight:bold;
	display: block;
	color:#FFFFFF;
}
.dashboard_drk_left {
	background: #2f3343;
	min-height: 450px;
	width: 20%;
	float: left;
}
.dashboard_drk_right {
	background: #3c3f4c;
	width: 80%;
	float: left;
	padding: 10px 15px;
	min-height: 500px;
}
.dashboard_drk_right ul { margin: 10px 15px; }
.dashboard_drk_right ul li { background: #252938; padding: 20px 20px; border-bottom: 1px solid #5b5e6a; margin-bottom: 10px; font-size: 16px;}
.dashboard_drk_right ul li a { color:#0c88ea; text-decoration: underline; font-weight: bold;}
.dashboard_drk_right ul li .status { float: right;}
.dashboard_drk_right ul li img { margin-left: 20px;}
.dashboard_drk_right h3 {
	margin: 15px 15px 20px 15px;
}
