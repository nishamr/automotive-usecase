}
.dashboard {
	background: #d5e8f0;
	min-height: 450px;
	width: 100%;
	position: relative;
}
.dashboard .user {
	background: url(../images/icon_user.png) no-repeat 30px 10px #b4d3e0;
	height: 92px;
	padding: 20px 0px 15px 110px;
	font-size: 12px;
}
.dashboard {
	font-size: 20px;
font-weight:bold;
	display: block;
}
.dashboard_left {
	background: #e6eef1;
	min-height: 450px;
	width: 20%;
	float: left;
}
.dashboard_right {
	background: #d5e8f0;
	width: 80%;
	float: left;
	padding: 10px 15px;
}
.dashboard_right h3 {
	margin: 15px 15px 20px 15px;
}
.dashboard_right label {
	display: block;
	font-size: 18px;
	color:#537888;
	font-weight:normal;
}
.dashboard_right input[type="text"] {
	display: block;
	margin-bottom: 20px;
	border-radius: 5px;
	border: none;
	padding: 10px 15px;
	font-size: 18px;
	width: 100%;
}
.dashboard_right input[type="file"] {
	display: block;
	margin-bottom: 20px;
	border-radius: 5px;
	border: none;
	padding: 10px 15px;
	font-size: 18px;
	background: #FFFFFF;
}
.dashboard_right input[type="date"] {
	display: block;
	margin-bottom: 20px;
	border-radius: 5px;
	border: none;
	padding: 10px 15px;
	font-size: 18px;
	width: 80%;
}
.dashboard_right input[type="submit"] {
	background: #0072bc;
	border: 1px solid #0072bc;
	color: #FFF;
	border-radius: 50px;
	padding: 8px 20px;
	width: 150px;
	font-size: 16px;
	margin: 15px;
}
.dashboard_right input[type="submit"]:hover {
	background: #FFF;
	border: 1px solid #0072bc;
	color: #0072bc;
}
.submit_block {
	border-top: 1px solid #000000;
	margin: 10px 15px 10px 15px;
}
