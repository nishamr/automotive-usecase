	float: left;
	width: 200px;
}
.supplier .left ul {
	position: relative;
	display: block;
	text-align: right;
	margin: 15px 0px 0px 0px;
}
.supplier .left ul li:nth-child(odd) a {
	color: #0c88ea;
	text-decoration: none;
	border-radius: 50px 0px 0px 50px;
	border: 0px;
	background: #FFFFFF;
	padding: 12px 20px;
